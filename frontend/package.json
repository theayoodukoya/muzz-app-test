{
  "name": "muzz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "test:watch": "vitest --watch --config vitest.config.ts",
    "test:coverage": "vitest --coverage --config vitest.config.ts",
    "test:component": "vitest --run src/**/*.test.tsx --config vitest.config.ts",
    "test:unit": "vitest --run src/**/*.test.ts --config vitest.config.ts",
    "test:integration": "vitest --run src/**/*.integration.test.ts --config vitest.config.ts",
    "test:all": "npm run test:unit && npm run test:component && npm run test:integration",
    "test:ci": "vitest --run --coverage --config vitest.config.ts",
    "clean": "rm -rf node_modules/.vite && rm -rf test-results && rm -rf playwright-report && rm -rf coverage",
    "clean:all": "rm -rf node_modules/.vite && rm -rf test-results && rm -rf playwright-report && rm -rf dist && rm -rf .next && rm -rf coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.4.0",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.79.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.10",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^24.0.0",
    "msw": "^2.6.4",
    "sass-embedded": "^1.89.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^5.4.10",
    "vitest": "^1.6.0",
    "@types/node": "^22.0.0"
  },
  "overrides": {
    "@testing-library/react": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0"
    }
  }
}
